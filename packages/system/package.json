{
  "name": "@gotpop/system",
  "version": "0.1.4",
  "description": "React design system components for gotpop",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*/index.d.ts",
      "default": "./dist/utils/*/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gotpop/gotpop-system.git",
    "directory": "packages/system"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "npx biome lint src/",
    "format:check": "npx biome check src/",
    "format:fix": "npx biome check --write src/",
    "type-check": "tsc --noEmit",
    "jsr:publish": "npx jsr publish",
    "jsr:publish:dry": "npx jsr publish --dry-run"
  },
  "dependencies": {
    "@next/font": "^14.2.15",
    "@storyblok/react": "^5.4.18",
    "react": "19.2.0",
    "react-icons": "^5.4.0",
    "server-only": "^0.0.1",
    "storyblok-rich-text-react-renderer": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/node": "^20.0.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "next": "16.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  }
}
