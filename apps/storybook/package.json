{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "description": "Storybook for gotpop design system components",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "preview": "serve storybook-static",
    "lint": "echo 'lint: no linting configured for storybook'",
    "format:check": "echo 'format:check: no formatting configured for storybook'",
    "format:fix": "echo 'format:fix: no formatting configured for storybook'",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf storybook-static"
  },
  "dependencies": {
    "@gotpop/system": "*",
    "storyblok-rich-text-react-renderer": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-links": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "storybook": "^8.4.6",
    "typescript": "^5",
    "vite": "^5.0.0"
  }
}
